# Symbiote Configuration
# Copy this file to .env and set your API keys

# Groq API key for LLaMA models
GROQ_API_KEY=your_groq_api_key_here

# Google API key for Gemini models
GOOGLE_API_KEY=your_google_api_key_here

# Optional: Default sass level (0-10)
DEFAULT_SASS_LEVEL=4

# Optional: Default model configurations
LLAMA_MODEL=llama-3.3-70b-versatile
GEMINI_MODEL=gemini-2.0-flash-exp

# Optional: Tool configurations
MAX_SHELL_TIMEOUT=300
MAX_FILE_SIZE_MB=10
